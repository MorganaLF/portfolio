.contact-list
  display table
  width 100%
  table-layout fixed
  break-word()
  &__row
    display table-row
  &__title
    display table-cell
    width 31%
    padding 0 rem(15px) rem(32px) 0
    font-size rem(21px)
    font-weight 600
    text-transform uppercase
    color accent-color
  &__text
    display table-cell
    padding-bottom rem(32px)
    font-size rem(21px)

